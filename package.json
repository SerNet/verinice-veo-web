{
  "author": "NBRX AG <info@nbrx.de>",
  "description": "veo-web",
  "engines": {
    "node": "20.16.x",
    "npm": "10.6.x"
  },
  "name": "veo-web",
  "private": true,
  "version": "1.16.0-SNAPSHOT",
  "scripts": {
    "cy:develop": "cypress run --env environment=develop",
    "cy-gui:develop": "cypress open --env environment=develop",
    "cy-gui:local": "cypress open --env environment=local",
    "cy:local": "cypress run --env environment=local",
    "dev": "nuxi dev",
    "dev:public": "nuxi dev --public",
    "e2e": "cypress run --env environment=local --browser firefox",
    "format:all": "prettier . --ignore-unknown --write",
    "format": "prettier --ignore-unknown --write",
    "format:check": "prettier . --check",
    "format:install-hook": "chmod u+x scripts/format-install-pre-commit-hook.sh && ./scripts/format-install-pre-commit-hook.sh",
    "generate": "nuxi generate",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --ignore-pattern 'public/paged.polyfill.js' .",
    "precommit": "npm run lint",
    "print": "docker run --add-host=host.docker.internal:host-gateway -v $(pwd)/print.js:/usr/src/app/print.mjs -v $(pwd)/dist:/usr/src/app/dist ghcr.io/drpayyne/chrome-puppeteer:latest node print.mjs http://host.docker.internal:3000/docs?print",
    "start": "nuxi start"
  },
  "dependencies": {
    "@casl/ability": "^6.3.1",
    "@codemirror/lang-json": "^6.0.1",
    "@mdi/font": "^7.0.0",
    "@mdi/js": "^7.0.96",
    "@nuxt/kit": "^3.0.0",
    "@tanstack/vue-query": "^4.37.1",
    "@toast-ui/editor": "^3.2.1",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.0.0",
    "@vueuse/core": "^11.0.0",
    "ajv": "^8.11.2",
    "ajv-formats": "^3.0.0",
    "chart.js": "^4.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "codemirror": "^6.0.1",
    "date-fns": "^3.0.0",
    "develop": "^1.0.0",
    "dompurify": "^3.1.5",
    "glob": "^11.0.0",
    "hash-sum": "^2.0.0",
    "intro.js": "^7.2.0",
    "js-cookie": "^3.0.1",
    "jsdom": "^24.0.0",
    "json-ptr": "^3.1.1",
    "jszip": "^3.10.1",
    "keycloak-js": "^25.0.0",
    "lodash": "^4.17.21",
    "nuxt-font-loader": "^2.2.2",
    "prismjs": "^1.29.0",
    "serve": "^14.2.0",
    "uuid": "^10.0.0",
    "vite-plugin-vuetify": "^2.0.1",
    "vue-chartjs": "^5.0.1",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.3.23",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "@nuxt/content": "^2.6.0",
    "@nuxt/eslint-config": "^0.5.0",
    "@nuxt/test-utils": "^3.12.0",
    "@nuxtjs/i18n": "8.3.3",
    "@types/glob": "^8.0.1",
    "@types/hash-sum": "^1.0.0",
    "@types/intro.js": "^5.1.0",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "cypress": "^13.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.24.1",
    "lint-staged": "^15.0.0",
    "npm": "^10.0.0",
    "nuxt": "^3.10.1",
    "prettier": "^3.2.5",
    "sass": "^1.77.8",
    "vitest": "^1.6.0"
  }
}
