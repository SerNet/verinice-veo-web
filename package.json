{
  "name": "veo-web",
  "version": "1.0.7",
  "engines": {
    "npm": "8.x",
    "node": "18.x"
  },
  "description": "veo-web",
  "author": "NBRX AG <info@nbrx.de>",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "print": "docker run --add-host=host.docker.internal:host-gateway -v $(pwd)/print.js:/usr/src/app/print.js -v $(pwd)/dist:/usr/src/app/dist ghcr.io/drpayyne/chrome-puppeteer:latest node print.js http://host.docker.internal:3000/docs?print"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node_modules/@nbrx/eslint-config-nuxt/commit-lint.js"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "eslint --fix"
    ]
  },
  "overrides": {
    "vue-i18n": "^9.3.0-beta.10"
  },
  "devDependencies": {
    "@casl/ability": "^6.3.1",
    "@codemirror/next": "^0.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "@mdi/font": "^6.1.95",
    "@mdi/js": "^7.0.96",
    "@nbrx/eslint-config-nuxt": "^1.1.0",
    "@nuxt/content": "^2.2.2",
    "@nuxtjs/i18n": "^8.0.0-beta.7",
    "@tanstack/vue-query": "4.20.4",
    "@toast-ui/editor": "^3.2.1",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@vueuse/core": "^9.6.0",
    "ajv": "^8.11.2",
    "ajv-formats": "^2.1.1",
    "chart.js": "^4.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^2.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "hash-sum": "^2.0.0",
    "husky": "^8.0.2",
    "intersection-observer": "^0.12.0",
    "intro.js": "^6.0.0",
    "json-ptr": "^3.1.1",
    "keycloak-js": "^20.0.1",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "nuxt": "3.0.0-rc.12",
    "nuxt-polyfill": "^1.0.3",
    "path-to-regexp": "^3.1.0",
    "prettier": "^2.8.1",
    "prismjs": "^1.29.0",
    "rehype-inline": "^2.0.0",
    "resize-observer": "^1.0.4",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.56.2",
    "uuid": "^9.0.0",
    "vue": "^3.2.45",
    "vue-chartjs": "^5.0.1",
    "vue-json-pointer": "^0.6.2",
    "vue-router": "^4.1.6",
    "vuedraggable": "^2.24.3",
    "vuetify": "^3.0.5",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.0/xlsx-0.19.0.tgz"
  }
