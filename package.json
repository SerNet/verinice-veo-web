    "vuex-simple": "^2.1.0",
    "vuex-typesafe-class": "^1.0.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.0",
    "ava": "^1.4.1"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register",
      "tsconfig-paths/register"
    ]
