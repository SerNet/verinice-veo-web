{
  "name": "veo-web",
  "version": "0.9.0",
  "engines": {
    "npm": "7.x",
    "node": "14.x"
  },
  "description": "veo-web",
  "author": "cpm systems <info@cpmsys.de>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "test:e2e": "cross-env LANG='de_DE.UTF-8' && cypress run",
    "test:e2e-open": "cross-env LANG='de_DE.UTF-8' && cypress open",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node_modules/@nbrx/eslint-config-nuxt/commit-lint.js"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "eslint"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "dependencies": {
    "@codemirror/next": "^0.14.0",
    "@mdi/font": "^6.1.95",
    "@mdi/js": "^6.1.95",
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/composition-api": "^0.29.2",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.0.0",
    "@toast-ui/vue-editor": "^3.1.0",
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "chart.js": "^3.6.0",
    "dotenv": "^10.0.0",
    "intersection-observer": "^0.12.0",
    "intro.js": "^4.3.0",
    "json-ptr": "^2.2.0",
    "keycloak-js": "10.x",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.27.0",
    "nuxt-i18n-composable": "^0.1.1",
    "nuxt-polyfill": "^1.0.3",
    "prismjs": "^1.25.0",
    "roboto-fontface": "^0.10.0",
    "uuid": "^8.3.2",
    "vee-validate": "^3.4.13",
    "vue-chart-3": "^0.5.11",
    "vue-json-pointer": "^0.6.2",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@cypress/code-coverage": "^3.9.11",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@nbrx/eslint-config-nuxt": "^1.1.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/chai": "^4.2.22",
    "@types/intl": "^1.2.0",
    "@types/intro.js": "^3.0.2",
    "@types/jest": "^27.0.2",
    "@types/json-schema": "^7.0.9",
    "@types/leaflet": "^1.7.5",
    "@types/lodash": "^4.14.175",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.1",
    "@types/prismjs": "^1.16.6",
    "@types/uuid": "^8.3.1",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.3",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.5.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-plugin-snapshots": "^1.4.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^7.18.0",
    "eslint-plugin-vuetify": "^1.0.1",
    "flush-promises": "^1.0.2",
    "husky": "^7.0.2",
    "istanbul-lib-coverage": "^3.0.1",
    "jest": "^27.2.3",
    "jest-junit": "^12.3.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^11.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "vue-jest": "^3.0.7"
  }
}
