    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/vuetify": "^0.5.5",
    "@types/json-schema": "^7.0.3",
    "babel-polyfill": "^6.26.0",
    "basic-auth": "^2.0.1",
    "cookie-universal-nuxt": "^2.0.14",
    "dotenv": "^7.0.0",
    "http-server": "^0.11.1",
    "interactjs": "^1.3.4",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nuxt": "^2.6.0",
    "public-ip": "^3.0.0",
    "request-ip": "^2.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
