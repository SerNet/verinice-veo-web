{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "declaration": false,
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [],
    "types": [
      "@types/node",
      "@types/lodash",
      "@types/jest",
      "jest",
      "@nuxt/types",
      "@nuxt/content",
      "@nuxtjs/i18n",
      "./types/context",
      "vuetify/types",
      "./types/vue-json-pointer",
      "./types/introjs"
    ]
  },
  "exclude": [
    "coverage",
    ".nuxt",
    ".gitlab",
    "dist",
    "cypress"
  ]
}
