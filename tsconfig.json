{
  // https://v3.nuxtjs.org/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["vitest/importMeta"],
    "baseUrl": ".",
    "paths": {
      "~": ["."],
      "~~": ["."],
      "~/*": ["./*"],
      "~~/*": ["./*"],
      "@": ["."],
      "@@": ["."],
      "@/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
